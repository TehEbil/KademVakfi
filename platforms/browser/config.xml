<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cycrosoft.kademvakfi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>KademVakfi</name>
    <description>
        KademVakfi
    </description>
    <author email="can.friedhoff@gmail.com" href="http://google.de">
        Cycrosoft
    </author>
    <icon height="180" platform="ios" src="icon-60@3x.png" width="180" />
    <icon src="icon.png" />
    <content src="index.html" />
    <access origin="*" subdomains="true" />
    <access origin="*" />
    <allow-navigation href="*" />
